<%- include('admin_header'); -%>
<div class="view-feedback-heading">
    <h3 class="text-center"> <%= mentor_name %>'s Webinars </h3>
</div>
<div class="view-feedback-outer-container">
<% webinars.forEach((webinar)=>{ %>
    <div class="view-feedback-inner-container">
        <div class="row container-box">
            <div class="col-md-4 section-1">
                <div class="row col-md-12">
                    <img src="/student_images/noimage.png" alt="mentor-image" class="view-attendance-img">
                </div>
                <p class="text-center"> <%= webinar.title %></p>
                <div class="text-center col-md-12">
                    <a href="/admin/view-feedback/<%= mentor_name %>/<%= webinar.title%>" class="btn btn-danger">View FeedBack</a>
                </div>
            </div>
            <div class="col-md-8 section-2">
                <div class="row">
                    <div class="col-md-12">
                        <h3>Webinar Details</h3>
                    </div>
                    <hr>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <h4> Webinar Title : </h4>
                        <hr>
                        <p> <%= webinar.title%> </p>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <h4> Mentor Scheduled : </h4>
                        <hr>
                        <p> <%= webinar.mentor %> </p>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <h4> Webinar Date : </h4>
                        <hr>
                        <p> <%= webinar.date %> </p>
                    </div>
                    <div class="col-md-6">
                        <h4> Webinar Time : </h4>
                        <hr>
                        <p> <%= webinar.time %> </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
<% }); %>
<div class="bottom-btn row">
    <a href="/admin/view-feedback/" class="btn btn-danger">Back To Mentors.</a>
</div>
</div>

<%- include('footer'); -%>