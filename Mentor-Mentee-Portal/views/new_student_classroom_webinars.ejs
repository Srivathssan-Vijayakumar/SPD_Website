<%- include('new_student_header'); -%>
<div class="container-fluid admin-page">
    <div id="mySidenav" class="sidenav">
        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
        <ul class=" top-navigation-area">
            <li class="nav-items"><a href="/student/classroom"><svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="#fff" class="bi bi-arrow-left-square" viewBox="0 0 16 16">
                <path fill-rule="evenodd" d="M15 2a1 1 0 0 0-1-1H2a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V2zM0 2a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V2zm11.5 5.5a.5.5 0 0 1 0 1H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5H11.5z"/>
              </svg></a></li>
        </ul>
        <ul class=" middle-navigation-area">            
            <li class="nav-items"><a href="/student/classroom/<%= classroom%>/test"><svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="#fff" class="bi bi-clipboard-check" viewBox="0 0 16 16">
                <path fill-rule="evenodd" d="M10.854 7.146a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708 0l-1.5-1.5a.5.5 0 1 1 .708-.708L7.5 9.793l2.646-2.647a.5.5 0 0 1 .708 0z"/>
                <path d="M4 1.5H3a2 2 0 0 0-2 2V14a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2V3.5a2 2 0 0 0-2-2h-1v1h1a1 1 0 0 1 1 1V14a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1V3.5a1 1 0 0 1 1-1h1v-1z"/>
                <path d="M9.5 1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-3a.5.5 0 0 1-.5-.5v-1a.5.5 0 0 1 .5-.5h3zm-3-1A1.5 1.5 0 0 0 5 1.5v1A1.5 1.5 0 0 0 6.5 4h3A1.5 1.5 0 0 0 11 2.5v-1A1.5 1.5 0 0 0 9.5 0h-3z"/>
              </svg></a></li>
            <li class="nav-items"><a href="/student/classroom/<%= classroom%>/webinars"><svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="#fff" class="bi bi-camera-video" viewBox="0 0 16 16">
                <path fill-rule="evenodd" d="M0 5a2 2 0 0 1 2-2h7.5a2 2 0 0 1 1.983 1.738l3.11-1.382A1 1 0 0 1 16 4.269v7.462a1 1 0 0 1-1.406.913l-3.111-1.382A2 2 0 0 1 9.5 13H2a2 2 0 0 1-2-2V5zm11.5 5.175 3.5 1.556V4.269l-3.5 1.556v4.35zM2 4a1 1 0 0 0-1 1v6a1 1 0 0 0 1 1h7.5a1 1 0 0 0 1-1V5a1 1 0 0 0-1-1H2z"/>
              </svg></a></li>
        </ul>
        <ul class=" bottom-navigation-area">
            <li class="nav-items"><a href="/logout"><svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="#fff" class="bi bi-box-arrow-left" viewBox="0 0 16 16">
                <path fill-rule="evenodd" d="M6 12.5a.5.5 0 0 0 .5.5h8a.5.5 0 0 0 .5-.5v-9a.5.5 0 0 0-.5-.5h-8a.5.5 0 0 0-.5.5v2a.5.5 0 0 1-1 0v-2A1.5 1.5 0 0 1 6.5 2h8A1.5 1.5 0 0 1 16 3.5v9a1.5 1.5 0 0 1-1.5 1.5h-8A1.5 1.5 0 0 1 5 12.5v-2a.5.5 0 0 1 1 0v2z"/>
                <path fill-rule="evenodd" d="M.146 8.354a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L1.707 7.5H10.5a.5.5 0 0 1 0 1H1.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3z"/>
              </svg></a></li>   
        </ul>
    </div>
    <div class="col-md-12 nav-button-area">
        <span onclick="openNav()"><svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-list" viewBox="0 0 16 16">
            <path fill-rule="evenodd" d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z"/>
        </svg></span>
        <div class="logo-container col-md-1">
            <img src="/images/logo.png" class="logo"/>
        </div>
    </div>
    <div id="admin-content col-md-12 main">
        <div class="row">
            <% if(webinars && webinars.length>0 ){ %>
                <div class="col-md-10 mentor-tests-container">
                    <%webinars.forEach((webinar) =>{ %>
                        <div class="outer-container">
                        <div class="row">
                            <div class="mentor-test-card col-md-12">
                                <h3 class="test-title"><svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-camera-video" viewBox="0 0 16 16">
                                    <path fill-rule="evenodd" d="M0 5a2 2 0 0 1 2-2h7.5a2 2 0 0 1 1.983 1.738l3.11-1.382A1 1 0 0 1 16 4.269v7.462a1 1 0 0 1-1.406.913l-3.111-1.382A2 2 0 0 1 9.5 13H2a2 2 0 0 1-2-2V5zm11.5 5.175 3.5 1.556V4.269l-3.5 1.556v4.35zM2 4a1 1 0 0 0-1 1v6a1 1 0 0 0 1 1h7.5a1 1 0 0 0 1-1V5a1 1 0 0 0-1-1H2z"/>
                                  </svg>&nbsp;&nbsp;<%= webinar.Title %></h3>
                                <button id="<%= webinar.Title%>" class="btn btn-outline-dark test-card-button" onclick="onclickfunc()">View Details </button>
                            </div>
                        </div>
                        <div  class="row">
                            <div id="<%= webinar.Title%>-details"class="col-md-12 mentor-test-details">
                                <div class="row">
                                    <div class="title col-md-6">
                                        Scheduled Date : 
                                    </div>
                                    <div class="value col-md-6">
                                        <%= webinar.Date %>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="title col-md-6">
                                        Scheduled Time : 
                                    </div>
                                    <div class="value col-md-6">
                                        <%= webinar.Time %>
                                    </div>
                                </div>
                                <div class="row">
                                    <a href="<%= webinar.WebinarLink %>"class="text-center col-md-6 webinar-button btn btn-outline-dark">
                                         Attend Webinar
                                    </a>
                                    <% if(!webinar.IsMarked){ %>
                                        <a href="/"class="text-center col-md-6 webinar-button btn btn-secondary disabled">
                                            Attendance Not Marked
                                       </a>
                                    <% }else{ %>
                                        <a href="/" class="text-center col-md-6 webinar-button btn btn-secondary disabled" >
                                            Attendance Marked
                                       </a>
                                    <% } %>
                                </div>
                            </div>
                        </div>
                        </div>
                    <% }); %>
                </div>
            <% } else {%>
                <div class="col-md-6 m-auto">
                    <h2 class="text-center">
                        No Webinars Added!!
                    </h2>
                </div>
            <% } %>
        </div>
    </div>
</div>

<script>
    function openNav() {
        document.getElementById("mySidenav").style.width = "150px";
      }
      function closeNav() {
        document.getElementById("mySidenav").style.width = "0";
      } 
    var onclickfunc;
      $(document).ready(()=>{
            onclickfunc =()=>{
                event.preventDefault()
                event.stopPropagation()
                var id=event.target.id+'-details'
                var details = $("#"+id)
                details.slideToggle(
                    2000,
                    'swing'
                )
            }
        })
</script>
