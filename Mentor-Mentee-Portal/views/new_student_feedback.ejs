<%- include('new_student_header'); -%>
<div class="container-fluid admin-page">
    <div id="mySidenav" class="sidenav">
        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
        <ul class=" top-navigation-area">
            <li class="nav-item"><div class="profile-image-container"><svg class="profile-image col-md-1" xmlns="http://www.w3.org/2000/svg" width="100" height="100" fill="#fff" class="bi bi-person-circle" viewBox="0 0 16 16">
                <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z"/>
                <path fill-rule="evenodd" d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8zm8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1z"/>
              </svg></div></li>
            <% if(student){ %>
                <li class="nav-item"><a href="/student">Welcome <%= student.Username%></a></li>
            <% }else{ %>
                <li class="nav-item"><a href="/student">Guest Login</a></li>
            <% } %>
        </ul>
        <ul class=" middle-navigation-area">
            <li class="nav-item"><a class="Active" href="/student/feedback">Give Feedback</a></li>
            <li class="nav-item"><a href="/student/profile">View Profile</a></li>
        </ul>
        <ul class=" bottom-navigation-area">
            <li class="nav-item"><a href="/logout">Logout</a></li>   
        </ul>
    </div>
    <div class="col-md-12 nav-button-area">
        <span onclick="openNav()"><svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-list" viewBox="0 0 16 16">
            <path fill-rule="evenodd" d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z"/>
        </svg></span>
        <div class="logo-container col-md-1">
            <img src="/images/logo.png" class="logo"/>
        </div>
    </div>
    <div id="admin-content col-md-12 main">
        <div class="student-feedback-form row">
            <form  action="/student/feedback" method="POST">
                <div class="row form-group">
                    <input class="form-control col-md-12" type="text" name="Student" value="<%= student.Name%>" readonly/>
                </div>
                <div class="row form-group">
                    <input class="col-md-12 form-control" name="mentor" type="text" placeholder="Enter Your Mentor" value="<%= mentor.Name%>" readonly/>
                </div>
                <div class="row form-group">
                    <select name="webinar" class="col-md-12 form-control">
                        <% webinars.forEach((webinar)=>{ %>
                            <option class="Feedback-mentor-option" value="<%= webinar.Title %>"><%= webinar.Title %></option>
                        <% }); %>
                    </select>
                </div>
                <div class="row form-group">
                    <textarea placeholder="Your Feedback" class="form-control col-md-12" type="text" name="feedback"></textarea>
                </div>
                <div class="row form-group">
                    <p> Your Rating : </p>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" name="rating" id="inlineRadio1" value="1">
                        <label class="form-check-label" for="inlineRadio1">1(Very Poor)</label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" name="rating" id="inlineRadio2" value="2">
                        <label class="form-check-label" for="inlineRadio2">2(Not Bad)</label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" name="rating" id="inlineRadio3" value="3" >
                        <label class="form-check-label" for="inlineRadio3">3(Average)</label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" name="rating" id="inlineRadio4" value="4" >
                        <label class="form-check-label-light" for="inlineRadio4">4(Good)</label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" name="rating" id="inlineRadio5" value="5" >
                        <label class="form-check-label" for="inlineRadio5">5(Excellent)</label>
                    </div>
                </div>
                <div class="row form-group">
                    <button type="submit" class="btn btn-outline-dark">
                        Submit Feedback
                    </button>
                </div>
            </form>
        </div>
    </div>
</div>
<script>
    function openNav() {
        document.getElementById("mySidenav").style.width = "350px";
      }
      function closeNav() {
        document.getElementById("mySidenav").style.width = "0";
      } 
</script>


